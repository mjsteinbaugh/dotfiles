#!/bin/sh

# """
# Shared shell configuration file.
# Updated 2021-04-25.
#
# Please do not modify this file on a shared server.
# 
# See also:
# - https://github.com/mjsteinbaugh/dotfiles
# """

# koopa shell
# https://koopa.acidgenomics.com/
XDG_CONFIG_HOME="${XDG_CONFIG_HOME:-${HOME}/.config}"
if [ -f "${XDG_CONFIG_HOME}/koopa/activate" ]
then
    # > export KOOPA_MINIMAL=1
    # > export KOOPA_TEST=1
    # shellcheck source=/dev/null
    . "${XDG_CONFIG_HOME}/koopa/activate"
fi
